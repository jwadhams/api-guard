{
    "name": "jwadhams/api-guard",
    "abandoned": "laravel/sanctum",
    "description": "A slightly archaic way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/jwadhams/api-guard",
    "authors": [
        {
            "name": "Chris Bautista",
            "email": "chrisbjr@gmail.com"
        },
        {
            "name": "Mark Jayson Fuentes",
            "email": "awkwardusername@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "JWadhams\\ApiGuard\\": "src"
        }
    },
    "minimum-stability": "stable"
}
